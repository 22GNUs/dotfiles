{
  "$schema": "https://opencode.ai/config.json",
  "autoupdate": false,
  "small_model": "synthetic/hf:Qwen/Qwen3-Coder-480B-A35B-Instruct",
  "compaction": {
    "auto": true,
    "prune": true,
  },
  "plugin": [
    // https://github.com/backnotprop/plannotator
    "@plannotator/opencode@latest",
  ],
  // models.dev: https://models.dev/
  "provider": {
    "foxcode": {
      "npm": "@ai-sdk/anthropic",
      "name": "Foxcode",
      "options": {
        "baseURL": "https://code.newcli.com/claude/aws/v1",
      },
      "models": {
        "claude-opus-4-5": {
          "id": "claude-opus-4-5-20251101",
          "name": "Claude Opus 4.5 (latest)",
          "family": "claude-opus",
          "attachment": true,
          "reasoning": false,
          "tool_call": true,
          "temperature": true,
          "release_date": "2025-11-24",
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
          "cost": {
            "input": 0.073,
            "output": 0.15,
            "cache_read": 0.003,
            "cache_write": 0.0372,
          },
          "limit": {
            "context": 150000,
            "output": 48000,
          },
        },
      },
    },
  },
  "agent": {
    "typescript-developer": {
      "model": "synthetic/hf:zai-org/GLM-4.7",
    },
    "java-developer": {
      "model": "synthetic/hf:zai-org/GLM-4.7",
    },
    "explore": {
      "model": "synthetic/hf:MiniMaxAI/MiniMax-M2.1",
    },
    "general": {
      "model": "synthetic/hf:MiniMaxAI/MiniMax-M2.1",
    },
    "compaction": {
      "model": "synthetic/hf:zai-org/GLM-4.7",
    },
  },
  "command": {},
  "mcp": {
    "context7": {
      "type": "remote",
      "url": "https://mcp.context7.com/mcp",
      "headers": {
        "CONTEXT7_API_KEY": "{env:CONTEXT7_API_KEY}",
      },
    },
    "gh_grep": {
      "type": "remote",
      "url": "https://mcp.grep.app",
    },
  },
}
